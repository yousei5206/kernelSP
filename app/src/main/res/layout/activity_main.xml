<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">

    <FrameLayout
        android:id ="@+id/fragmentConsole"
        android:name ="kernelsp.maeda.is.kyusan_u.ac.jp.kernelsp.ConsoleActivity"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/fragmentExecution"
        android:orientation="horizontal"
        android:name ="kernelsp.maeda.is.kyusan_u.ac.jp.kernelsp.ExecutionActivity" />


    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:transitionName="kernelsp.maeda.is.kyusan_u.ac.jp.kernelsp.MemActivity"
        android:id="@+id/fragmentMem">

    </FrameLayout>





</LinearLayout>